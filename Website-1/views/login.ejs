    <div class="page1">
      <div class="container">
        <div class="glass">
          <div class="title">
            <span>Login</span>
          </div>
          <div class="form-container">
            <div class="form">
              <form id="form" action="/users/login" method="POST">
                <div class="form-group">
                  <div class="form-field" id="email">
                    <div class="icon">
                      <i class="fas fa-user"></i>
                    </div>
                    <div class="input gmail">
                      <input type="email" placeholder="Email" onfocus="addBoxShadow('email')" onblur="removeBoxShadow('email')"
                      name="email" style="cursor: none;" id="email" autocomplete="off"
                      value="<%= typeof email != 'undefined' ? email : '' %>" />
                    </div>
                  </div>
                  <div class="form-field" id="password">
                    <div class="icon">
                      <i class="fas fa-lock"></i>
                    </div>
                    <div class="input password">
                      <input type="password" placeholder="Password" onfocus="addBoxShadow('password')"
                        onblur="removeBoxShadow('password')" style="cursor: none;" name="password" id="pass" autocomplete="off"
                        value="<%= typeof password != 'undefined' ? password : '' %>" />
                    </div>
                    <div class="check">
                      <i class="far fa-eye-slash"></i>
                    </div>
                  </div>
                </div>
                <div id="forgot" style="cursor: none">
                  <small>Forgot password?</small>
                </div>
                <button type="submit" id="submit" style="cursor: none; outline: 0 none;">Login</button>
              </form>
            </div>
            <div class="divider">
              <span class="text">or</span>
            </div>
            <div class="web-log">

            </div>
          </div>
        </div>
      </div>
    </div>

    <style>
      * {
        margin: 0;
        padding: 0;
        font-family: "Poppins";
      }

      html,
      body {
          height: 100%; 
          width: 100%;
          overflow: hidden;  
      }

      html {
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
      }

      body {
        cursor: none;
        font-family: "Poppins";
        border: none
      }

      :root {
        --primary-light: #8abdff;
        --primary: #6d5dfc;
        --primary-dark: #5b0eeb;
        --white: #FFFFFF;
        --greyLight-1: #E4EBF5;
        --greyLight-2: #c8d0e7;
        --greyLight-3: #bec8e4;
        --greyDark: #9baacf;
        --Light: #E4EBF5;
        --Dark: #232429;
        --Glass-light: linear-gradient(to right bottom, #ffaf87, #ff687e);
        --Glass-dark: linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        --Glass-light-login: rgba(255, 255, 255, 0.5);
        --Glass-dark-login: rgba(0, 0, 0, 0.5);
        --Glass-light-login-g: linear-gradient(to right bottom, rgba(255, 255, 255, 0.55), rgba(255, 255, 255, 0.5));
        --Glass-dark-login-g: linear-gradient(to right bottom, rgba(0, 0, 0, 0.55), rgba(0, 0, 0, 0.5));
        --purple: linear-gradient(to right, #805bdb, #4e45f0);
        --peace: linear-gradient(to right, #ffccc1, #cae7e5);
      }

      .background {
        height: 100vh;
        width: 100%;
        display: flex;
        overflow: hidden;
      }

      .background.light {
        background: var(--Light);
      }

      .background.dark {
        background: var(--Dark);
      }

      .background .messages .alert {
        padding: 20px 40px;
        min-width: 420px;
        position: absolute;
        right: 0;
        top: 100px;
        overflow: hidden;
        border-radius: 5px;
      }
      .background.light .messages .alert {
        background: rgba(255,255,255, 0.5);
        border-image: linear-gradient(to bottom, #ffaf87, #ff687e) 2 100%;
        }
      .background.dark .messages .alert {
        background: rgba(0, 0, 0, 0.5);
        border-image: linear-gradient(to bottom, #051821, #1a4645, #266867, #379291, turquoise) 2 100%;
      }

      .background .messages .alert.show {
        animation: show_slide 1s ease forwards;
      }
      @keyframes show_slide {
        0%{
            transform: translateX(100%);
        }
        40%{
            transform: translateX(-10%);
        }
        80%{
            transform: translateX(0%);
        }
        100%{
            transform: translateX(-10px);
        }
      }
      .background .messages .alert.hide{
        animation: hide_slide 1s ease forwards;
      }
      @keyframes hide_slide {
        0%{
            transform: translateX(-10px);
        }
        40%{
            transform: translateX(0%);
        }
        80%{
            transform: translateX(-10%);
        }
        100%{
            transform: translateX(100%);
        }
      }
      .background .messages .alert .fa-exclamation-circle {
        position: absolute; 
        left: 20px;
        top: 50%;
        transform: translateY(-50%);
        font-size: 25px;
      }
      .background.light .messages .alert .fa-exclamation-circle {
        background: -moz-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: -webkit-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: linear-gradient(to right bottom, #ffaf87, #ff687e);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent;
      }
      .background.dark .messages .alert .fa-exclamation-circle {
        background: -moz-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: -webkit-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent; 
      }
      
      .background .messages .alert .fa-key {
        position: absolute; 
        left: 20px;
        top: 50%;
        transform: translateY(-50%);
        font-size: 25px;
      }
      .background.light .messages .alert .fa-key {
        background: -moz-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: -webkit-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: linear-gradient(to right bottom, #ffaf87, #ff687e);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent;
      }
      .background.dark .messages .alert .fa-key {
        background: -moz-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: -webkit-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent;
      }

      .background .messages .alert .msg {
        padding: 0 20px;
        font-size: 18px;
      }
      .background.light .messages .alert .msg {
        color: #ff687e;
      }
      .background.dark .messages .alert .msg {
        color: #40e0d0;
      }

      .background .messages .alert .close-btn {
        position: absolute;
        right: 1px;
        top: 50%;
        transform: translateY(-50%);
        padding: 20px 18px;
      }
      .background.light .messages .alert .close-btn {
        background: rgba(255,255,255, 0.6);
      }
      .background.dark .messages .alert .close-btn {
        background: rgba(0,0,0, 0.6);
      }

      .background.light .messages .alert .close-btn:hover {
        background: rgba(255,255,255, 0.8);
      }
      .background.dark .messages .alert .close-btn:hover {
        background: rgba(0,0,0, 0.8);
      }

      .background .messages .alert .close-btn .fa-times {
        font-size: 22px;
        line-height: 40px;
      }
      .background.light .messages .alert .close-btn .fa-times {
        background: -moz-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: -webkit-linear-gradient(to right bottom, #ffaf87, #ff687e);
        background: linear-gradient(to right bottom, #ffaf87, #ff687e);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent;
      }
      .background.dark .messages .alert .close-btn .fa-times {
        background: -moz-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: -webkit-linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background: linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        -webkit-background-clip: text;
        -moz-background-clip: text;
        background-clip: text-box;
        -webkit-text-fill-color: transparent;
      }

      .background .nav-bar button {
        position: fixed;
        height: 20px;
        width: 20px;
        border: none;
        border-radius: 50px;
        z-index: 5;
        transition: all 0.4s ease;
      }

      .background .nav-bar .theme {
        top: 15px;
        right: 20px;
      }

      .background.light .nav-bar button {
        border: solid 3px transparent;
        background-image: linear-gradient(rgba(255, 255, 255, 0), rgba(255, 255, 255, 0)), linear-gradient(to right bottom, #ffaf87, #ff687e);
        background-origin: border-box;
        background-clip: content-box, border-box;
        box-shadow: 2px 1000px 1px var(--Light) inset;
      }

      .background.light .nav-bar button:hover {
        box-shadow: none;
      }

      .background.dark .nav-bar button {
        border: solid 3px transparent;
        background-image: linear-gradient(rgba(255, 255, 255, 0), rgba(255, 255, 255, 0)), linear-gradient(to right bottom, #051821, #1a4645, #266867, #379291, turquoise);
        background-origin: border-box;
        background-clip: content-box, border-box;
        box-shadow: 2px 1000px 1px var(--Dark) inset;
      }

      .background.dark .nav-bar button:hover {
        box-shadow: none;
      }

      .background .nav-bar button i {
        transition: all 0.4s ease;
      }

      .background .nav-bar .theme i {
        font-size: 21px;
      }

