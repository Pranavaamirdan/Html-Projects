    <script>
      const theme = document.querySelector(".theme");
      theme.addEventListener("click", () => {
        if ($(".background").hasClass("light")) {
          $(".background").removeClass('light');
          $(".background").addClass('dark');
          if ($('.fa').hasClass('fa-moon')) {
            $('.fa').removeClass("fa-moon");
            $('.fa').addClass("fa-sun");
          }
        } else if ($(".background").hasClass("dark")) {
          $(".background").removeClass('dark');
          $(".background").addClass('light');
          if ($('.fa').hasClass('fa-sun')) {
            $('.fa').removeClass("fa-sun");
            $('.fa').addClass("fa-moon");
          }
        }
      });

      const next = document.querySelector(".next");
      next.addEventListener("click", () => {
        if (!$('.glass').hasClass('login')) {
          $('.glass').addClass('login');
          $('.glass-overlay').addClass('login');
          $(".intro").addClass('login');
        }
      });

      const check = document.querySelector(".check");
      const password = document.querySelector("#pass");
      check.addEventListener("click", () => {
        if ($('.far').hasClass('fa-eye-slash')) {
          $('.far').removeClass("fa-eye-slash");
          $('.far').addClass("fa-eye");
          password.type = "text"
        } else if ($('.far').hasClass('fa-eye')) {
          $('.far').removeClass("fa-eye");
          $('.far').addClass("fa-eye-slash");
          password.type = "password"
        }
      })
    </script>


