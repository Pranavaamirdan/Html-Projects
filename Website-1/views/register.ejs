* {
    margin: 0;
    padding: 0;
}

html,
body {
    height: 100%; 
    width: 100%;
    overflow: hidden;  
}

.Topbar {
  position: absolute;
  right: 35px;
  top: 20px;
  background: transparent;
  margin-left: 10px;
  z-index: 100;
}
.Topbar li {
  list-style: none;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  padding: 15px 20px 15px 20px;
  background: rgba(255, 255, 255,0.5);
  border-radius: 50px;
}

.Topbar li:hover {
  background : rgba(255, 255, 255,0.8);
}
.Topbar li i{
  padding: 3px 3px 3px 3px;
  font-size: 19px;
  color: rgb(24,24,24);
} 
.Topbar li a{
  height: 100%;
  width: 100%;
  border-radius: 12px;
  align-items: center;
  text-decoration: none;
  transition: all 0.4s ease;
  color: rgb(24,24,24);
  border-bottom: rgba(255, 255, 255,0.5);
}

body {
    background : linear-gradient(to right bottom,#051821,#1a4645,#266867,#379291,turquoise);
    cursor: none;
    margin: 0;
    display: flex;
    font-family: "Poppins";
}

#form-container {
    margin: auto;   
    width: 900px;
    height: 530px;
    background: white;
    display: flex;
    border-radius: 4px;
    box-shadow: 0px 0px 15px #cbced1;/*, -13px -13px 20px #fff;*/
    z-index: 5;
}

#background {
    background: linear-gradient(to right bottom,#ffaf87,#ff687e);/*url("../Images/provon.png") no-repeat;*/
    background-size: cover;
    background-position-y: 50%;
    background-position-x: 50%;
    width: 90%;
    height: 100%;
}

.container {
      width: 250px;
      position: relative;
      display: flex;
      left: 75px;
      top: 150px;
      right: 75px;
      justify-content: space-between;
    }

    .container .card {
      position: relative;
      width: 400px;
      height: 180px;
    }

    .container .card .face{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      transition: 0.5s;
    }

    .container .card .face.face1 {
      background: transparent;
      display: flex;
      justify-content: center;
      align-items: center;
      z-index: 1;
      transform-origin: bottom;
    }

    .container .card svg text {
      margin: 10px 0 0;
      color: white;
      text-align: center;
    }

    .container .card:hover .face.face1 {
      transform: translateY(-100%) rotateX(90deg);
      background: rgba(255, 255, 255,0.1);
      transition: 500ms;
    }

    .container .card .face.face2 {
      background: transparent;
      justify-content: center;
      align-items: center;
      padding: 20px;
      /* padding-top: 7px;
      padding-left: 13px;
      padding-right: 13px; */
      transform-origin: top;
      transform: translateY(100%) rotateX(90deg);
    }

    .container .card:hover .face.face2 {
      transform: translateY(0) rotateX(0deg);
    }


#form-section {
    width: 100%;
    height: 100%;
    background-color: white;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding: 10px;
    box-sizing: border-box;
}

#form-section form {
    box-sizing: border-box;
    width: 100%;
    padding: 10px;
}

.form-group {
    display: flex;
    flex-direction: column;
}

input {
    outline: 0 none;
    border: 0 none;
    padding: 5px 15px;
    width: 100%;
    height: 30px;
}

.form-field {
    display: flex;
    align-items: center;
    padding: 10px 15px;
    /*box-shadow: 0px 0px 15px 5px rgba(0, 0, 0, 0.15);*/
    box-shadow: inset 8px 8px 8px #cbced1,inset -8px -8px 8px #fff;
    margin: 5px;
    border-radius: 10px;
    transition: 200ms box-shadow ease-in-out;
    border: 0.1px solid rgba(0, 0, 0, 0.15);
}

.form-field i {
    color: rgba(65,65,65);
}

#login {
    font-size: 15px;
    text-align: center;
    color: black;
    padding: 20px 0px;
}

#login a {
    color: rgba(0, 119, 108);
    text-decoration: underline;
} 

#login a:hover {
    color: #ff687e;
    text-decoration: underline;
}

#submit {
    width: 100%;
    padding: 7.9px;
    border: none;
    border-radius: 10px;
    outline: none;
    background: linear-gradient(to right bottom,#051821,#1a4645,#266867,#379291,turquoise);
    font-family: "Poppins";
    margin: 5px 2px;
}

#submit:hover {
    background: linear-gradient(to right bottom,#ffaf87,#ff687e);
    transition: 200ms background ease-in-out;
}

#submit:active {
    background: #ffaf87;
    transition: 200ms background ease-in-out;
}

#icon-container {
    display: flex;
    justify-content: center;
    border-top: 1px solid rgb(54, 57, 63, 0.15);
    padding: 15px;
}

.icons {
    width: 50px;
    height: 50px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24px;
    border-radius: 50%;
    color: white;
    box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.5);
    margin: 7.5px
}

